/**app.wxss**/
page{
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  height: 100%;
  background-color: #ebebeb;
  overflow-y: auto;
  }
view{
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
image{
    width: 100%;
    height: 100%;
}
/*star*/
.star{
  width:100rpx;
  height: 100rpx;
  border-radius: 50%;
  box-shadow:0 6rpx 10rpx rgba(148,148,148,0.5);
  margin: 0 0 40rpx 605rpx;
  position: relative;
  background: #ef6461 url(data:image/png;base64,) no-repeat center center/38rpx auto;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.btn{
  width:700rpx;
  height: 80rpx;
  color:#f4f8fb;
  font-size:36rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  background-color: #ef6461;
}
.button-hover{
  background-color:#df4542 ;
}
.button-disable{
  background-color:#ef7d7b ;
}
button::after{display: none;}
.icon-male{
  background:url(data:image/png;base64,) no-repeat;
  background-size:24rpx auto ;
}
.icon-female{
  background:url(data:image/png;base64,) no-repeat;
   background-size:24rpx auto ;
}

.navigator-hover{background-color: transparent;}
.tabbar{
    display: flex;
    height: 90rpx;
    display:-webkit-flex;
    background-color: #fcfcfc;
    padding: 0 80rpx;
    justify-content: space-between;
    box-shadow:0 2rpx 24rpx rgba(187,187,187,0.5);
    position: relative;
}
.nav{
    display: inline-block;
    width: 200rpx;
    text-align: center;
    font-size: 28rpx;
    line-height: 90rpx;
    color:#919191 ;
    position: relative;
}
.current{font-size: 30rpx;color:#ef6461 ;}
.current:after{
    content: '';
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 200rpx;
    height: 6rpx;
    background-color: #ef6461;
}