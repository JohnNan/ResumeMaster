/* pages/send/send.wxss */
.send-wrap{
    width: 100%;
    height:100%;
    background-color: #fff;
    padding:0 25rpx;
}
.send-bg{
    width: 100%;
    height: 504rpx;
    background: url(data:image/png;base64,) no-repeat center 220rpx;
    background-size: 262rpx auto;
}
.send-wrap input{
    display: block;
    height: 86rpx;
    border:1px solid #c9c9c9;
    margin-bottom: 50rpx;
    font-size: 34rpx;
    color: #999;
    text-align: center;
}
.send-wrap .btn{
    width:100%;
    height: 86rpx;
    border-radius: 6rpx;
    color: #fff;
    font-size: 34rpx;
    line-height: 86rpx;
}