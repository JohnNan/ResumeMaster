/* pages/me/me.wxss */
.me{
    padding-top:34rpx;
}
.card{
    width: 100%;
    margin-bottom: 32rpx;
    position: relative;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.person{
    width: 700rpx;
    height: 307rpx;
     border-radius:10rpx;
    background-color: #fff;
    margin:0 25rpx 40rpx;
    padding: 50rpx 0;
    display: flex;
    justify-content: space-between;
    flex-wrap: nowrap;
    position: relative;
}
.card-left{
    text-align: center;
    padding-left: 40rpx;
    padding-right: 60rpx;
}
.card-right{
    flex: 1;
}
.mugshot{
    display: inline-block;
    width: 150rpx;
    height: 150rpx;
    margin-bottom:26rpx;
}
.mugshot image{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}
.name{
    font-size: 34rpx;
    color: #333;
}
.name .icon{
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-left:12rpx;
}
.view-p{
    font-size: 30rpx;
    line-height: 1;
    color: #333;
    margin-bottom: 30rpx;
}
.view-span{
    display: inline-block;
    min-width: 110rpx;
    font-size: 28rpx;
    color: #999;
    margin-right: 45rpx;
}
.overflow{
    display: inline-block;
    width: 260rpx;
    white-space:nowrap;
    text-overflow:ellipsis;
    overflow: hidden;
}
.myresume{
    height: auto;
    background-color: #fff;
    padding:0 20rpx;
    margin-bottom: 40rpx;
}
.myresume image{
    width:192rpx;
    height: 140rpx;
    margin-left: 20rpx;
    float: left;
}
.card-cntt{
    position: relative;
}
.mytemplate{
    padding: 30rpx 0;
    position: relative;
    z-index: 2;
    background-color: #fff;
    border-top: 1px solid #d9d9d9;
}
.arrow{
    display: block;
    position: absolute;
    right: 0;
    top:0;
    width: 88rpx;
    height: 88rpx;
    background:url(data:image/png;base64,) no-repeat center center/30rpx auto;
}

.mytemplate:after{
    content: '';
    display: table;
    clear: both;
}
.title{
    line-height:36rpx;
    font-size: 28rpx;
    color: #333;
    padding:30rpx 0;
    overflow: hidden; 
}
.myresume .view-p{
    margin-left: 256rpx;
    font-size: 32rpx;
    color: #333;
    padding-top: 15rpx;
    overflow: hidden;
}
.myresume .view-span{
    min-width: 0;
    font-size: 24rpx;
    color: #ef6461;
    margin-right: 100rpx;
    float: right;
    position: relative;
    top: 2px;
}
.myresume .time{
    margin-left: 256rpx;
    margin-bottom: 0;
    font-size: 24rpx;
    color: #bbb;
    line-height: 2;
    background:url(data:image/png;base64,) no-repeat left center;
    background-size: 22rpx auto;
    padding:0 0 0 35rpx;
}
.title .icon{
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 36rpx auto;
    float: left;
    margin-left: 20rpx;
    margin-right: 15rpx;
}
.title .favorite{
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 36rpx auto;
}

/*androidmask*/
.mask-cntt{
    width:547rpx;
    height: 175rx;
    border-radius: 6rpx;
    background-color: #fff;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}
.mask-text{
    font-size:30rpx;
    color: #333;
    padding: 30rpx 40rpx;
}
.mask-text:first-of-type{border-bottom:1px solid #dfdfdf;}

.member-list{
    width: 100%;
    height: 350rpx;
    background-color: #fff;
    padding:0 20rpx;
    position: absolute;
    left: 0;
    bottom: 0;
}
.member-tit{
    text-align: center;
    padding:30rpx 0;
    font-size:32rpx;
    color: #333;
    border-bottom:1px solid #d9d9d9 ;
    margin-bottom: 15rpx;
}
.flexbox{
    display: flex;
    justify-content: space-around;
    align-content: stretch;
    padding: 21rpx 0;
}
.flexitem{
    flex: 1;
    text-align: center;
    font-size:28rpx;
    line-height: 1;
    color: #333;
}
.dot{
    width: 80rpx;
    height: 75rpx;
    background: url(data:image/png;base64,) no-repeat 32rpx 40rpx;
    background-size: 8rpx auto;
    position: absolute;
    right: 0;
    top: 0;
}
